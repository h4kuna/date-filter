{
  "name": "h4kuna/date-filter",
  "type": "library",
  "description": "Add option define own filters based on date for latte.",
  "homepage": "https://github.com/h4kuna/date-filter",
  "license": "MIT",
  "authors": [
    {
      "name": "Milan Matějček",
      "email": "milan.matejcek@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-intl": "*",
    "h4kuna/memoize": "^0.1.3",
    "nette/di": "^3.0",
    "nette/utils": "^3.0 || ^4.0"
  },
  "require-dev": {
    "latte/latte": "^3.0",
    "nette/application": "^3.1",
    "nette/tester": "^2.1",
    "phpstan/phpstan": "^1.9",
    "phpstan/phpstan-strict-rules": "^1.4",
    "tracy/tracy": "^2.6"
  },
  "autoload": {
    "psr-4": {
      "h4kuna\\DateFilter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "h4kuna\\DateFilter\\Tests\\": "tests/src/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "phpstan": "vendor/bin/phpstan analyse",
    "tests": "vendor/bin/tester -s -j 4 --colors 1 -s -C tests/src"
  }
}
